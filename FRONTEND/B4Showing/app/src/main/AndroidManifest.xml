<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kr.or.hanium.probono.little_trio.b4showing">

    <!-- 웹뷰 인터넷 접근 허용 -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".network.ApplicationController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".BeaconRegisterActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".StationFind.StationFindActivity" />
        <activity android:name=".SeatInfo.SeatInfoActivity" />
        <activity android:name=".TestActivity" />
    </application>

</manifest>